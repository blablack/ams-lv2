@prefix doap: 	<http://usefulinc.com/ns/doap#> .
@prefix foaf: 	<http://xmlns.com/foaf/0.1/> .
@prefix lv2: 	<http://lv2plug.in/ns/lv2core#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: 	<http://ll-plugins.nongnu.org/lv2/namespace#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .

<http://github.com/blablack/ams-lv2/vcf/gui>
  a guiext:GtkUI ;
  guiext:binary <vcf_gui.so> .

<http://github.com/blablack/ams-lv2/vcf>
	a lv2:Plugin, lv2:SpectralPlugin ;
	lv2:project <http://github.com/blablack/ams-lv2/> ;
	doap:name "AMS VCF" ;
	doap:license <http://opensource.org/licenses/isc> ;
	ll:pegName "p" ;
	guiext:ui <http://github.com/blablack/ams-lv2/vcf/gui> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port 
		[
		a lv2:InputPort, lv2:AudioPort;
		lv2:index 0 ; 
		lv2:symbol "in" ;
		lv2:name "In" ;
	] ,	[
		a lv2:InputPort, lv2:CVPort ;
		lv2:index 1 ; 
		lv2:symbol "freqPort" ;
		lv2:name "Frequency Mod" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
	        lv2:maximum 1.0 ;
	] ,	[
		a lv2:InputPort, lv2:CVPort ;
		lv2:index 2 ; 
		lv2:symbol "expFM" ;
		lv2:name "Exp FM" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
	        lv2:maximum 1.0 ;
	] ,	[
	  	a lv2:InputPort, lv2:CVPort ;
		lv2:index 3 ; 
		lv2:symbol "linFM" ;
		lv2:name "Lin FM" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
	        lv2:maximum 1.0 ;
	] , 	[
	  	a lv2:InputPort, lv2:CVPort ;
		lv2:index 4 ; 
		lv2:symbol "resonancePort" ;
		lv2:name "Resonance Mod" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
	        lv2:maximum 1.0 ;
	] , 	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ; 
		lv2:symbol "vcfType" ;
		lv2:name "VCF Type" ;
		lv2:default 0;
		lv2:minimum 0;
		lv2:maximum 7;
		lv2:portProperty lv2:enumeration ;
		lv2:scalePoint
				[
			    	rdfs:label "Resonant Lowpass" ;
			   	rdf:value 0 ;
			] ,	[
				rdfs:label "Lowpass" ;
				rdf:value 1 ;
			] ,	[
		  		rdfs:label "Highpass" ;
				rdf:value 2 ;
			] , 	[
		  		rdfs:label "Bandpass I" ;
				rdf:value 3 ;
			] ,	[
		  		rdfs:label "Bandpass II" ;
				rdf:value 4 ;
			] ,	[
		  		rdfs:label "Notch" ;
				rdf:value 5 ;
			] , 	[
		  		rdfs:label "24 dB Lowpass I" ;
				rdf:value 6 ;
			] , 	[
		  		rdfs:label "24 dB Lowpass II" ;
				rdf:value 7 ;
			]
	] , 	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ; 
		lv2:symbol "inputGain" ;
		lv2:name "Input Gain" ;
		lv2:default 1 ;
		lv2:minimum 0 ;
		lv2:maximum 10 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ; 
		lv2:symbol "freq" ;
		lv2:name "Frequency" ;
		lv2:default 5 ;
		lv2:minimum 0 ;
		lv2:maximum 10 ;
	] , 	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 8 ; 
		lv2:symbol "expFMGain" ;
		lv2:name "Exp. FM Gain" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 10 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 9 ; 
		lv2:symbol "linFMGain" ;
		lv2:name "Lin. FM Gain" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 10 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 10 ; 
		lv2:symbol "resonance" ;
		lv2:name "Resonance" ;
		lv2:default 0.8 ;
		lv2:minimum 0.01 ;
		lv2:maximum 1.0 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 11 ; 
		lv2:symbol "resonanceGain" ;
		lv2:name "Resonance Gain" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] ,	[
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 12 ; 
		lv2:symbol "out" ;
		lv2:name "Out" ;
	] .
