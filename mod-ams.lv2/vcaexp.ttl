@prefix doap: 	<http://usefulinc.com/ns/doap#> .
@prefix foaf: 	<http://xmlns.com/foaf/0.1/> .
@prefix lv2: 	<http://lv2plug.in/ns/lv2core#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: 	<http://ll-plugins.nongnu.org/lv2/namespace#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .

<http://github.com/blablack/ams-lv2/vcaexp/gui>
  a guiext:GtkUI ;
  guiext:binary <vcaexp_gui.so> .

<http://github.com/blablack/ams-lv2/vcaexp>
	a lv2:Plugin, lv2:AmplifierPlugin ;
	lv2:project <http://github.com/blablack/ams-lv2/> ;
	doap:name "AMS VCA Exp" ;
	doap:license <http://opensource.org/licenses/isc> ;
	ll:pegName "p" ;
	guiext:ui <http://github.com/blablack/ams-lv2/vcaexp/gui> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort, lv2:CVPort ;
		lv2:index 0 ; 
		lv2:symbol "gain1Data" ;
		lv2:name "Main Gain" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0 ;
	] ,	[
		a lv2:InputPort, lv2:CVPort ;
		lv2:index 1 ; 
		lv2:symbol "gain2Data" ;
		lv2:name "2nd Gain" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0 ;
	] ,	[
		a lv2:InputPort, lv2:AudioPort ;
		lv2:index 2 ; 
		lv2:symbol "in1Data" ;
		lv2:name "In 1" ;
	] ,	[
	  	a lv2:InputPort, lv2:AudioPort;
		lv2:index 3 ; 
		lv2:symbol "in2Data" ;
		lv2:name "In 2" ;
	] , [
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ; 
		lv2:symbol "gain1" ;
		lv2:name "Gain Offset" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ; 
		lv2:symbol "gain2" ;
		lv2:name "2nd Gain Boost" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] , [
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ; 
		lv2:symbol "in1" ;
		lv2:name "In 1 Level" ;
		lv2:default 1 ;
		lv2:minimum 0 ;
		lv2:maximum 2 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ; 
		lv2:symbol "in2" ;
		lv2:name "In 2 Level" ;
		lv2:default 1 ;
		lv2:minimum 0 ;
		lv2:maximum 2 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 8 ; 
		lv2:symbol "outputLevel" ;
		lv2:name "Output Level" ;
		lv2:default 1 ;
		lv2:minimum 0 ;
		lv2:maximum 2 ;
	] ,	[
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 9 ; 
		lv2:symbol "out" ;
		lv2:name "Out" ;
	] .
