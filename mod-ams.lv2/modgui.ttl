@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://github.com/blablack/ams-lv2/vcaexp>
    modgui:gui [
        modgui:resourcesDirectory <modgui/vcaexp> ;
        modgui:iconTemplate <modgui/vcaexp/mod_ams_vca_exp.html> ;
        modgui:stylesheet <modgui/vcaexp/mod_ams_vca_exp.css> ;
        modgui:screenshot <modgui/vcaexp/mod_ams_vca_exp_ss.png> ;
        modgui:thumbnail <modgui/vcaexp/mod_ams_vca_exp_tn.png> ;

        modgui:port [
            lv2:index 0 ;
            lv2:symbol "gain1" ;
            lv2:name "Gain Offset" ;
        ] ,
        [
            lv2:index 1 ;
            lv2:symbol "gain2" ;
            lv2:name "2nd Gain Boost" ;
        ] ,
        [
            lv2:index 2 ;
            lv2:symbol "in1" ;
            lv2:name "In 1 Level" ;
        ] ,
        [
            lv2:index 3 ;
            lv2:symbol "in2" ;
            lv2:name "In 2 Level" ;
        ] ,
        [
            lv2:index 4 ;
            lv2:symbol "outputLevel" ;
            lv2:name "Output Level" ;
        ] ;
    ] .

<http://github.com/blablack/ams-lv2/vcalin>
    modgui:gui [
        modgui:resourcesDirectory <modgui/vcalin> ;
        modgui:iconTemplate <modgui/vcalin/mod_ams_vca_lin.html> ;
        modgui:stylesheet <modgui/vcalin/mod_ams_vca_lin.css> ;
        modgui:screenshot <modgui/vcalin/mod_ams_vca_lin_ss.png> ;
        modgui:thumbnail <modgui/vcalin/mod_ams_vca_lin_tn.png> ;

        modgui:port [
            lv2:index 0 ;
            lv2:symbol "gain1" ;
            lv2:name "Gain Offset" ;
        ] ,
        [
            lv2:index 1 ;
            lv2:symbol "gain2" ;
            lv2:name "2nd Gain Boost" ;
        ] ,
        [
            lv2:index 2 ;
            lv2:symbol "in1" ;
            lv2:name "In 1" ;
        ] ,
        [
            lv2:index 3 ;
            lv2:symbol "in2" ;
            lv2:name "In 2" ;
        ] ,
        [
            lv2:index 4 ;
            lv2:symbol "outputLevel" ;
            lv2:name "Output Level" ;
        ] ;
    ] .
